#!/usr/bin/env bash
echo "#################################"
echo "Start xelphlinux-before"
echo "#################################"

echo "Populate Core Team keys"
echo "#################################"
pacman-key --init
pacman-key --populate archlinux
pacman-key --populate chaotic

echo "#################################"
echo "End xelphlinux-before"
echo "#################################"
rm -rf /usr/local/bin/xelphlinux-before
